{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:dev": {
      "dependsOn": ["^build:dev"],
      "outputs": ["dist/**", ".next/**"]
    },
    "build:prod": {
      "dependsOn": ["^build:prod"],
      "outputs": ["dist/**", ".next/**"]
    },
    "deploy:dev": {
      "dependsOn": [
        "build:dev"
      ]
    },
    "deploy:prod": {
      "dependsOn": [
        "build:prod"
      ]
    },
    "lint": {
      "cache": false
    },
    "fix": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}